{
  "name": "listing-todos",
  "displayName": "listing-todo",
  "description": "Generate a list of the todo's so you can have an overview of them",
  "version": "0.0.1",
  "publisher": "kristofriebbels",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.listTodos"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.listTodos",
        "title": "List Todos"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.3.4",
    "vscode": "^1.0.0"
  }
}
